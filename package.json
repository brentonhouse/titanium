		"id": "com.appcelerator.titanium.cli"
	},
	"name": "titanium",
	"description": "Appcelerator Titanium Command line",
	"keywords": [
		"appcelerator",
		"titanium",
		"mobile",
		"ios",
		"iphone",
		"android",
		"blackberry",
		"html5",
		"mobileweb",
		"mobile web",
		"appc-client"
	],
	"version": "5.2.1",
	"author": {
		"name": "Appcelerator, Inc.",
		"email": "npmjs@appcelerator.com"
	},
	"maintainers": [
		{
			"name": "Jeff Haynie",
			"email": "jhaynie@appcelerator.com"
		},
		{
			"name": "Chris Barber",
			"email": "cbarber@appcelerator.com"
		}
	],
	"bugs": {
		"url": "https://jira.appcelerator.org/browse/TIMOB"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/appcelerator/titanium.git"
	},
	"preferGlobal": true,
	"dependencies": {
		"adm-zip": "0.4.13",
		"async": "^3.1.0",
		"colors": "1.3.3",
		"fields": "0.1.24",
		"fs-extra": "^7.0.1",
		"humanize": "0.0.9",
		"longjohn": "0.2.12",
		"moment": "^2.24.0",
		"node-appc": "^0.2.49",
		"request": "2.88.0",
		"semver": "6.1.1",
		"sprintf": "0.1.5",
		"temp": "0.9.0",
		"winston": "1.1.2"
	},
	"devDependencies": {
		"@seadub/danger-plugin-dependencies": "0.1.0",
		"@seadub/danger-plugin-eslint": "^1.0.0",
		"@seadub/danger-plugin-junit": "0.1.1",
		"danger": "^8.0.0",
		"eslint": "^5.16.0",
		"eslint-config-axway": "^4.2.2",
		"eslint-plugin-mocha": "^5.3.0",
		"husky": "^2.3.0",
		"lint-staged": "^8.1.5",
		"mocha": "^6.1.3",
		"mocha-jenkins-reporter": "^0.4.1",
		"nyc": "^14.0.0",
		"should": "^13.2.1"
	},
	"license": "Apache-2.0",
	"bin": {
		"ti": "./bin/titanium",
		"titanium": "./bin/titanium"
	},
	"main": "./lib/titanium",
	"directories": {
		"lib": "./lib"
	},
	"scripts": {
		"lint": "eslint .",
		"test": "npm run lint && npm run unit-test",
		"unit-test": "JUNIT_REPORT_PATH=junit_report.xml nyc mocha tests/**/test-*.js --reporter=mocha-jenkins-reporter"
	},
	"mocha": {
		"timeout": 3000,
		"check-leaks": true,
		"globals": [
			"Hyperloop",
			"HyperloopObject"
		]
	},
	"nyc": {
		"check-coverage": true,
		"lines": 62,
		"statements": 61,
		"functions": 47,
		"branches": 50,
		"exclude": [
			"tests/**/*.js",
			"locales/**/*.js"
		],
		"reporter": [
			"cobertura",
			"text"
		]
	},
	"engines": {
		"node": ">=4.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": "eslint"
	}
}
